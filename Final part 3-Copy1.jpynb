    "import pandas as pd\n",
    "import numpy as np\n",
    "data = pd.read_csv('wind.txt', delim_whitespace=True, parse_dates=[[0, 1, 2]])\n",
    "data.columns = ['date', 'RPT', 'VAL', 'ROS', 'KIL', 'SHA', 'BIR', 'DUB', 'CLA', 'MUL', 'CLO', 'BEL', 'MAL']\n",
    "data.set_index('date', inplace=True)\n",
    "def fix_year(year):\n",
    "    if year > 2000:\n",
    "        year -= 100\n",
    "    return year\n",
    "\n",
    "data.index = data.index.map(lambda x: x.replace(year=fix_year(x.year)))\n",
    "data.index = pd.to_datetime(data.index)\n",
    "missing_values = data.isnull().sum()\n",
    "total_non_missing = data.notnull().sum().sum()\n",
    "mean_windspeed = data.mean().mean()\n",
    "loc_stats = pd.DataFrame({\n",
    "    'min': data.min(),\n",
    "    'max': data.max(),\n",
    "    'mean': data.mean(),\n",
    "    'std': data.std()\n",
    "})\n",
    "day_stats = pd.DataFrame({\n",
    "    'min': data.min(axis=1),\n",
    "    'max': data.max(axis=1),\n",
    "    'mean': data.mean(axis=1),\n",
    "    'std': data.std(axis=1)\n",
    "})\n",
    "jan_avg = data[data.index.month == 1].mean()\n",
    "yearly_data = data.resample('Y').mean()\n",
    "monthly_data = data.resample('M').mean()\n",
    "weekly_data = data.resample('W').mean()\n",
    "weekly_stats = data.loc['1961-01-02':'1961-01-08'].resample('W-Mon').agg(['min', 'max', 'mean', 'std'])[:52]\n"
   ]
  },
